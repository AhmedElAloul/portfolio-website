<%- include('./partials/header', {data: {pageTitle} }); %>
    <%- include('./partials/navbar', {data: {pageTitle} }); %>

        <main>
            <section class="contact-section">
                <div class="container my-4">
                    <h2 class="section-heading mb-5 text-center">
                        Contact <span>Me</span>
                    </h2>
                    <div class="row row-cols-1 row-cols-md-2">
                        <div class="col box-1 order-2 order-md-1">
                            <h2 class="mb-4">
                                Let's Start A Project Together!
                            </h2>
                            <p>
                                Have Any project on mind? Let's start together to make it live. Don't like form? <a
                                    href="mailto:ahmedaloul22@gmail.com">send email.</a>
                            </p>
                            <img src="images/contact.png" alt="" class="img-fluid" style="max-width: 400px;">
                        </div>
                        <div class="col box-2 mb-5 mb-md-0  order-1 order-md-2">
                            <form id="form">
                                <div class="form-group mb-4">
                                    <label for="">What Is Your Name ? <sup>*</sup></label>
                                    <input type="text" name="name" class="form-control" required>
                                </div>
                                <div class="form-group mb-4">
                                    <label for="">What Is Your Email Address ? <sup>*</sup></label>
                                    <input type="email" name="email" class="form-control" required>
                                </div>
                                <div class="form-group mb-4">
                                    <label for="">What Is Your Budget Range ? <sup>*</sup></label>
                                    <input type="text" name="budget" placeholder="Ex - 500 - 2500 $"
                                        class="form-control" required>
                                </div>
                                <div class="form-group mb-4">
                                    <label for="">Project Brief (Tell Me About Your Project) <sup>*</sup></label>
                                    <textarea name="projectBrief" cols="30" rows="5" class="form-control"
                                        placeholder="Ex - I have a figma design you have to convert it into html. iâ€™m attaching figma design link. Let me know total time and price"></textarea>
                                </div>
                                <div class="form-group mb-4">
                                    <button class="btn btn-primary-outline ms-auto d-block">Submit Form</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>
        </main>

        <script src="js/contact.js"></script>

        <%- include('./partials/footer'); %>